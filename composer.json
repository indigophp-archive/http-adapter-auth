{
    "name": "indigophp/http-adapter-auth",
    "description": "Easily authenticate requests in Ivory Http Adapter",
    "license": "MIT",
    "keywords": ["auth", "http-adapter"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "beberlei/assert": "~2.3",
        "egeloen/http-adapter": "0.7.*"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\HttpAdapter\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
